main:    
	addi $a1, $zero, 15
	j mult
	jr $ra


#multiplication code
mult:
	add   $v1, $zero, $zero   # init to 0

loopstart:
	beq   $a0, $a1, fin
	add   $v1, $v1, $a1
	addi  $a0, $a0, 1
	j     loopstart

fin:           
  	jr  $ra

